generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Leaderboard {
  id        Int      @id @default(autoincrement())
  username  String   @unique
  score     Int
  createdAt DateTime @default(now())
}
